@model UsuarioModel

@{
    ViewData["Title"] = "Registro de usuario";
}

<h1>Registro de usuario</h1>
<form method="post" class="from-floating">
    <div class="form-group">
        <label asp-for="Nombre"></label>
        <input class="from-control" asp-fro=Nombre/>
    </div>
    <div class="form-group">
        <label asp-for="Fecha"></label>
        <input class="from-control" asp-fro=Fecha/>
    </div>
    <div class="form-group">
        <label asp-for="Correo"></label>
        <input class="from-control" asp-fro=Correo/>
    </div>
    <br />
    <div class="text-center panel-body">
        <button type="submit" class="btn btn-sm btn-primary">Registrar</button>
    </div>
</form>